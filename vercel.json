{
  "version": 2,

  "name": "midasconsulting.org",

  "builds": [{ "src": "out", "use": "@vercel/node" }],

  "routes": [{ "src": "/(.*)", "dest": "/index.js" }],

  "functions": {
    "api/index.js": {
      "memory": 3008,

      "maxDuration": 40
    },

    "api/*.js": {
      "memory": 3008,

      "maxDuration": 40
    }
  }
}
